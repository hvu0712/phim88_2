<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">
    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@+id/start"
        motion:duration="2000">

        <KeyFrameSet>
            <KeyAttribute
                android:translationX="@dimen/dp_minus_400"
                motion:framePosition="0"
                motion:motionTarget="@id/viewOne"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationX="@dimen/dp_8"
                motion:framePosition="50"
                motion:motionTarget="@id/viewOne"
                motion:transitionEasing="decelerate" />

            <KeyAttribute
                android:translationX="@dimen/dp_minus_8"
                motion:framePosition="55"
                motion:motionTarget="@id/viewOne"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationX="0dp"
                motion:framePosition="60"
                motion:motionTarget="@id/viewOne"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                motion:framePosition="65"
                motion:motionTarget="@id/viewOne"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@android:color/black" />
            </KeyAttribute>

            <KeyAttribute
                motion:framePosition="100"
                motion:motionTarget="@id/viewOne"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@color/color_primary" />
            </KeyAttribute>

            <KeyAttribute
                android:translationX="@dimen/dp_400"
                motion:framePosition="0"
                motion:motionTarget="@id/viewTwo"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationX="@dimen/dp_minus_8"
                motion:framePosition="50"
                motion:motionTarget="@id/viewTwo"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationX="@dimen/dp_8"
                motion:framePosition="55"
                motion:motionTarget="@id/viewTwo"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationX="0dp"
                motion:framePosition="60"
                motion:motionTarget="@id/viewTwo"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                motion:framePosition="0"
                motion:motionTarget="@id/viewTwo"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@android:color/black" />
            </KeyAttribute>

            <KeyAttribute
                motion:framePosition="65"
                motion:motionTarget="@id/viewTwo"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@android:color/black" />
            </KeyAttribute>

            <KeyAttribute
                motion:framePosition="100"
                motion:motionTarget="@id/viewTwo"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@color/color_primary" />
            </KeyAttribute>

            <KeyAttribute
                android:translationY="@dimen/dp_600"
                motion:framePosition="0"
                motion:motionTarget="@id/viewThree"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationY="@dimen/dp_minus_8"
                motion:framePosition="50"
                motion:motionTarget="@id/viewThree"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationY="8dp"
                motion:framePosition="55"
                motion:motionTarget="@id/viewThree"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationY="0dp"
                motion:framePosition="60"
                motion:motionTarget="@id/viewThree"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                motion:framePosition="0"
                motion:motionTarget="@id/viewThree"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@android:color/black" />
            </KeyAttribute>

            <KeyAttribute
                motion:framePosition="65"
                motion:motionTarget="@id/viewThree"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@android:color/black" />
            </KeyAttribute>

            <KeyAttribute
                motion:framePosition="100"
                motion:motionTarget="@id/viewThree"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@color/color_primary" />
            </KeyAttribute>

            <KeyAttribute
                android:translationY="@dimen/dp_minus_600"
                motion:framePosition="0"
                motion:motionTarget="@id/viewFour"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationY="8dp"
                motion:framePosition="50"
                motion:motionTarget="@id/viewFour"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationY="-8dp"
                motion:framePosition="55"
                motion:motionTarget="@id/viewFour"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:translationY="0dp"
                motion:framePosition="60"
                motion:motionTarget="@id/viewFour"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                motion:framePosition="0"
                motion:motionTarget="@id/viewFour"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@android:color/black" />
            </KeyAttribute>

            <KeyAttribute
                motion:framePosition="65"
                motion:motionTarget="@id/viewFour"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@android:color/black" />
            </KeyAttribute>

            <KeyAttribute
                motion:framePosition="100"
                motion:motionTarget="@id/viewFour"
                motion:transitionEasing="accelerate">
                <CustomAttribute
                    motion:attributeName="backgroundColor"
                    motion:customColorValue="@color/color_primary" />
            </KeyAttribute>

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="0"
                motion:motionTarget="@id/view"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="60"
                motion:motionTarget="@id/view"
                motion:transitionEasing="accelerate" />

            <KeyAttribute
                android:alpha="1"
                motion:framePosition="100"
                motion:motionTarget="@id/view"
                motion:transitionEasing="accelerate" />

        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/view"
            android:layout_width="@dimen/dp_200"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/viewOne"
            android:layout_width="@dimen/dp_250"
            android:layout_height="@dimen/dp_2"
            android:layout_marginBottom="@dimen/dp_16"
            motion:layout_constraintBottom_toTopOf="@id/view"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />

        <Constraint
            android:id="@+id/viewThree"
            android:layout_width="@dimen/dp_2"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/viewTwo"
            motion:layout_constraintStart_toStartOf="@id/viewOne"
            motion:layout_constraintTop_toTopOf="@id/viewOne" />

        <Constraint
            android:id="@+id/viewFour"
            android:layout_width="@dimen/dp_2"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/viewTwo"
            motion:layout_constraintEnd_toEndOf="@id/viewOne"
            motion:layout_constraintTop_toTopOf="@id/viewOne" />

        <Constraint
            android:id="@+id/viewTwo"
            android:layout_width="@dimen/dp_250"
            android:layout_height="@dimen/dp_2"
            android:layout_marginTop="@dimen/dp_16"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/view" />

        <Constraint
            android:id="@+id/background"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/view"
            android:layout_width="@dimen/dp_200"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

        <Constraint
            android:id="@+id/viewOne"
            android:layout_width="@dimen/dp_250"
            android:layout_height="@dimen/dp_2"
            android:layout_marginBottom="@dimen/dp_16"
            motion:layout_constraintBottom_toTopOf="@id/view"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />

        <Constraint
            android:id="@+id/viewThree"
            android:layout_width="@dimen/dp_2"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/viewTwo"
            motion:layout_constraintStart_toStartOf="@id/viewOne"
            motion:layout_constraintTop_toTopOf="@id/viewOne" />

        <Constraint
            android:id="@+id/viewFour"
            android:layout_width="@dimen/dp_2"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="@id/viewTwo"
            motion:layout_constraintEnd_toEndOf="@id/viewOne"
            motion:layout_constraintTop_toTopOf="@id/viewOne" />

        <Constraint
            android:id="@+id/viewTwo"
            android:layout_width="@dimen/dp_250"
            android:layout_height="@dimen/dp_2"
            android:layout_marginTop="@dimen/dp_16"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/view" />

        <Constraint
            android:id="@+id/background"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />

    </ConstraintSet>

</MotionScene>
